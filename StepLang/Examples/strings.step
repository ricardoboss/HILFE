string foo = "hello world!"

println("foo: ", foo)
println("foo[0]: ", foo[0])
println("foo[6]: ", foo[6])

println("\"🤷‍♂️\"[0]: ", "🤷‍♂️"[0])
println("\"a🤷‍♂️\"[1]: ", "a🤷‍♂️"[1])

println("\"\\ta\": ", "\ta")

println("indexOf(foo, \"world\"): ", indexOf(foo, "world"))
println("indexOf(foo, \"bar\"): ", indexOf(foo, "bar"))
println("contains(foo, \"world\"): ", contains(foo, "world"))
println("contains(foo, \"bar\"): ", contains(foo, "bar"))

println("startsWith(foo, \"hello\"): ", startsWith(foo, "hello"))
println("startsWith(foo, \"world\"): ", startsWith(foo, "world"))
println("endsWith(foo, \"world!\"): ", endsWith(foo, "world!"))
println("endsWith(foo, \"hello\"): ", endsWith(foo, "hello"))

println("length(foo): ", length(foo))
println("length(\"Hello\"): ", length("Hello"))
println("length(\"\"): ", length(""))
println("length(\" \"): ", length(" "))
println("length(\"🤷‍♂️\"): ", length("🤷‍♂️"))
